{
  "name": "@bitkidd/adonis-logger-logdna",
  "version": "2.3.0",
  "description": "LogDNA driver for Adonis Logger",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "adonisjs",
    "logger",
    "logdna"
  ],
  "author": "keeross",
  "license": "MIT",
  "dependencies": {
    "logdna-winston": "^3.0.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/sink": "^1.0.17",
    "coveralls": "^3.0.6",
    "cz-conventional-changelog": "^3.0.2",
    "japa": "^3.0.0",
    "japa-cli": "^1.0.1",
    "nyc": "^14.1.1",
    "semver": "^7.1.3",
    "standard": "^14.1.0"
  },
  "standard": {
    "globals": [
      "use"
    ]
  },
  "nyc": {
    "exclude": [
      "bin"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keeross/adonis-logger-logdna.git"
  },
  "bugs": {
    "url": "https://github.com/keeross/adonis-logger-logdna/issues"
  },
  "homepage": "https://github.com/keeross/adonis-logger-logdna#readme"
}
